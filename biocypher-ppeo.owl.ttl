                                                               owl:unionOf ( xsd:float
                                                                             xsd:int
                                                                             xsd:string
                                                                           )
                                                             ]
                                           ] ;
                           rdfs:comment "A parameter of the environment which is represented by a name and a value."@en ;
                           rdfs:label "environment parameter"@en .


###  http://purl.org/ppeo/PPEO.owl#event
PPEO:event rdf:type owl:Class ;
           rdfs:subClassOf PPEO:technologicalConcept ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty PPEO:hasDateTime ;
                             owl:someValuesFrom xsd:dateTime
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty PPEO:hasType ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty PPEO:hasDescription ;
                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty PPEO:hasExternalIdentifier ;
                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange [ rdf:type rdfs:Datatype ;
                                               owl:unionOf ( xsd:anyURI
                                                             xsd:string
                                                           )
                                             ]
                           ] ;
           PPEO:hasRelatedSynonym "time factor"@en ;
           rdfs:comment "A discrete change to the state of one or more observation units that happened at a given point in time, be it deliberate or inadvertent."@en ;
           rdfs:label "event"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource PPEO:event ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty PPEO:hasExternalIdentifier ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange [ rdf:type rdfs:Datatype ;
                                           owl:unionOf ( xsd:anyURI
                                                         xsd:string
                                                       )
                                         ]
                       ] ;
   PPEO:hasOntologyRecommendation "http://www.cropontology.org/rdf/CO_715:0000006"^^xsd:anyURI
 ] .


