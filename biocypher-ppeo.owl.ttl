                         owl:onProperty PPEO:hasType ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange [ rdf:type rdfs:Datatype ;
                                           owl:unionOf ( xsd:anyURI
                                                         xsd:string
                                                       )
                                         ]
                       ] ;
   PPEO:hasOntologyRecommendation "http://www.cropontology.org/rdf/CO_715:0000003"^^xsd:anyURI
 ] .


###  http://purl.org/ppeo/PPEO.owl#factor
PPEO:factor rdf:type owl:Class ;
            rdfs:subClassOf PPEO:technologicalConcept ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty PPEO:hasModality ;
                              owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                              owl:onClass PPEO:factor_value
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty PPEO:hasType ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty PPEO:hasDescription ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ;
            rdfs:comment "A condition that varies between observation units, by taking different values or modalities, assessing the effect of which is the goal of the study. A factor may be biotic (pest, disease interaction) or abiotic (treatment, cultural practice) in nature. It must have at least two values or modalities."@en ;
            rdfs:label "factor"@en .


###  http://purl.org/ppeo/PPEO.owl#factor_value
PPEO:factor_value rdf:type owl:Class ;
                  rdfs:subClassOf PPEO:technologicalConcept ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty PPEO:isModalityOf ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass PPEO:factor
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty PPEO:hasValue ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                      owl:unionOf ( xsd:float
                                                                    xsd:int
                                                                    xsd:string
                                                                  )
                                                    ]
                                  ] ;
                  rdfs:comment "A value or modality of a factor."@en ;
                  rdfs:label "factor value"@en .


###  http://purl.org/ppeo/PPEO.owl#growth_facility
PPEO:growth_facility rdf:type owl:Class ;
                     rdfs:subClassOf PPEO:technologicalConcept ,
